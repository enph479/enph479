<Window x:Class="ElectricalToolSuite.FindAndReplace.DebugFindResult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      Name="DebugFindResults"
      Title="FindResults"
      Width="300"
      Height="600"
      d:DesignHeight="600"
      d:DesignWidth="300"
      mc:Ignorable="d">
    <Grid Background="White">
        <ListView HorizontalAlignment="Left" Height="auto" Margin="0,0,0,0" VerticalAlignment="Top" Width="auto" x:Name="ListOfMatchingElements" MouseDoubleClick="GetSelectedElementFromResults">
            <ListView.View>
                <GridView>
                    <GridViewColumn Width="150" Header="Family Name" DisplayMemberBinding ="{Binding Path=Symbol.Family.Name}"/>
                    <GridViewColumn Width="150" Header="Family Type" DisplayMemberBinding ="{Binding Path=Symbol.Name}"/>         
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>    
</Window>
